   "source": [
    "from tkinter import *\n",
    "\n",
    "\n",
    "# root\n",
    "root = Tk()\n",
    "\n",
    "\n",
    "frame = LabelFrame(root, text=\"This is a frame!\", padx=50, pady=50)\n",
    "frame.pack(padx=10, pady=10)\n",
    "\n",
    "\n",
    "button = Button(frame, text=\"Don't touch me!\")\n",
    "button.pack(padx=10, pady=10)\n",
    "\n",
    "\n",
    "# main loopevent\n",
    "root.mainloop()\n"
   ]
  }
